{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "antd": ["es"],
      "antd/es/*": ["es/*"],
      "antd/lib/*": ["lib/*"],
      "antd/locale/*": ["locale/*"],
      "gzj-ui": ["es"],
      "gzj-ui/es/*": ["es/*"],
      "gzj-ui/lib/*": ["lib/*"],
      "gzj-ui/locale/*": ["locale/*"]
    }
  },
  "include": ["components/*/demo/*.tsx"],
  "exclude": ["components/*/demo/_semantic*"]
}
